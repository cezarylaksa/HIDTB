{
  "name": "Have I Downloaded This Before?",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "minimum_chrome_version": "27.0.1448.0",
  "description": "Keep your downloads folder clean by preventing duplicates.",
  "permissions": [
    "downloads",
    "downloads.open",
    "notifications"
  ],
  "background": {
    "scripts": ["scripts/haveidownloadedthisbefore.js"]
  },
  "manifest_version": 2
}
